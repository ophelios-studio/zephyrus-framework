{
  "name": "ophelios/zephyrus-framework",
  "description": "Template project for Zephyrus foundation.",
  "license": "MIT",
  "authors": [
    {
      "name": "David Tucker",
      "email": "david.tucker@ophelios.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ophelios/zephyrus": "0.5.0",
    "twilio/sdk": "^6.0",
    "symfony/var-dumper": "*",
    "vlucas/phpdotenv": "^5.5",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "ext-zip": "*",
    "ext-posix": "*",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-ssh2": "*",
    "ext-gd": "*",
    "ext-libxml": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "10.*",
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "Models\\": "app/Models",
      "Controllers\\": "app/Controllers"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "nodejs-php-fallback/nodejs-php-fallback": true
    }
  }
}
